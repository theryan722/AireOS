Public Class Info

    ''' <summary>
    ''' The name of the operating system
    ''' </summary>
    ''' <returns>String, the name of the OS</returns>
    ''' <remarks></remarks>
    Public Shared Function OSName() As String
        Return "Aire OS"
    End Function

    ''' <summary>
    ''' The version of the operating system
    ''' </summary>
    ''' <returns>String, the version of the OS</returns>
    ''' <remarks></remarks>
    Public Shared Function OSVersion() As String
        Return "0.0.0.1"
    End Function

    ''' <summary>
    ''' The release info of the operating system
    ''' </summary>
    ''' <returns>String, the release info of the OS</returns>
    ''' <remarks></remarks>
    Public Shared Function OSRelease() As String
        Return "Official|Pre-Alpha"
    End Function

    ''' <summary>
    ''' The website of the operating system
    ''' </summary>
    ''' <returns>String, the website of the OS</returns>
    ''' <remarks></remarks>
    Public Shared Function Website() As String
        Return "https://github.com/theryan722/aireos"
    End Function

End Class
